<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.asysbang.developerheavn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".async.CodeAsyncTask"
            android:label="@string/code_async_async_task">
            <intent-filter>
                <action android:name="asysbang.action.CODE" />
                <category android:name="asysbang.category.CODE" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.CodePopupWindow"
            android:label="@string/code_ui_popup_window">
            <intent-filter>
                <action android:name="asysbang.action.CODE" />
                <category android:name="asysbang.category.CODE" />
            </intent-filter>
        </activity>

        <activity
            android:name=".manager.CodeManager"
            android:label="@string/code_manager">
            <intent-filter>
                <action android:name="asysbang.action.CODE" />
                <category android:name="asysbang.category.CODE" />
            </intent-filter>
        </activity>

        <activity
            android:name=".threadpooldemo.DemoThreadPool"
            android:label="@string/thread_pool">
            <intent-filter>
                <action android:name="asysbang.action.CODE" />
                <category android:name="asysbang.category.CODE" />
            </intent-filter>
        </activity>
    </application>

</manifest>
